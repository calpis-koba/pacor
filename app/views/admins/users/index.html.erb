<div class="container">
 <table  class="table table-striped">
  <tr>
   <td>ユーザネーム</td>

   <td></td>
  </tr>
 
   <% @users.each do |user| %>

    <tr>
     <td><%= link_to user.last_name,admins_user_path(user) %></td>

     <td>
      <%= link_to "詳細", admins_user_path(user), class: "btn btn-success" %>
      <%= link_to "削除", admins_user_path(user), method: :delete, data: { "confirm" => "本当に削除しますか？" }, class: "btn btn-danger" %>
     </td>
    </tr>

   <% end %>
   <%= paginate(@users) %>
  </table>
 </div>